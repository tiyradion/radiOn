<div>

  <a class="close">X</a>
  <h2 class="modal-title">
    All <% if(Radion.userType === "promoters") { %>Stations<% } %><% if(Radion.userType === "stations") { %>Promoters<% } %>
  </h2>

  <ul class="pure-menu-list">
    <% users.forEach(function(user) { %>
      <li class="pure-menu-item" data-user-id="<%- user.id %>">
        <a class="pure-menu-link"><a><%- user.name %></a><button type="button" class="btn-add-contact">+</button></a>
      </li>
    <% }) %>
  </ul>

</div>
