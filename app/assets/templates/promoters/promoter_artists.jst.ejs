<div class="dash-div">

  <div class="promo-artists">

    <div class="promo-artists-header">
      <h2 class="promo-contacts-h2 pure-menu-heading">My Artists</h2>
      <a class="add-artist open-modal">Add +</a>
    </div>

    <div class="promo-artists-container">

    <% artists.forEach(function(artist) { %>

    <div class="artist promo-artist">
      <input type="hidden" name="artist-id" data-artist-id="<%- artist.id %>">
      <div class="artist-target" >
        <input type="hidden" name="artist-id" data-artist-id="<%- artist.id %>">
        <div class="artist-name"><%- artist.name %><input type="hidden" name="artist-id" data-artist-id="<%- artist.id %>"><a class="remove remove-promoter-artist">X</a></div>
        <div class="artist-profile" style="background-image: url(<%- artist.picture_upload_1_file %>);"></div>
      </div>
    </div>

    <% }) %>

    </div>

  </div>

  <div class="artist-modal"><div>

</div>
