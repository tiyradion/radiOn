<div class="pure-menu custom-restricted-width">

  <div class="contact-header">
    <h2 class="contact-title pure-menu-heading">
      <% if(Radion.userType === "promoters") { %>
        Stations
      <% } else { %>
        Promoters
      <% } %>
    </h2>
    <button type="button" class="new-contact">Add +</button>
  </div>

  <ul class="all-contacts pure-menu-list">
    <li class="all-contact pure-menu-item">
      <a class="pure-menu-link all-notes active">All
        <% if(Radion.userType === "promoters") { %>
          Stations
        <% } else { %>
          Promoters
        <% } %>
      </a>
    </li>
    <% contacts.forEach(function(contact) { %>
      <li class="contact pure-menu-item">
        <a class="pure-menu-link single-note" data-contact-id="<%- contact.id %>"><%- contact.name %>
        <button type="button" class="remove-contact" data-contact-id="<%- contact.id %>">X</button>
        </a>
      </li>
    <% }) %>
  </ul>

</div>
