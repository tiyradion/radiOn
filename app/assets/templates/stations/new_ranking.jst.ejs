<div class="artists-header">
  <h2 class="pure-menu-heading">New Artists</h2>


<% artists.forEach(function(artist) { %>

<div class="artist station-artist" data-artist-id="<%- artist.id %>" data-artist-name="<%- artist.name %>" data-album-name="<%- artist.album_name %>">
  <a id="artist-route" class="ranking-artist">
      <div class="artist-name "><%- artist.album_name %> by <%- artist.name %></div>
      <div class="artist-profile " style="background-image: url(<%- artist.picture_upload_1_file %>);"></div>
  </a>
</div>

<% }) %>

</div>
